{
  "name": "ts-simple-boilderplate",
  "version": "0.0.1",
  "description": "Simple boilerplate for @sota1235",
  "main": "dist/index.js",
  "type": "module",
  "dependencies": {
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@sota1235/eslint-config": "5.0.5",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "ts-loader": "9.5.4",
    "typescript-eslint": "8.55.0",
    "vitest": "^4.0.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.publish.json",
    "build:publish": "tsc -p ./tsconfig.publish.json",
